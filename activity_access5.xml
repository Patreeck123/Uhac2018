<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".access1"
    android:background="#83a4ff"
    >

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="368dp"
        android:layout_height="549dp"
        android:layout_centerInParent="true"
        android:alpha="0.4"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/dogpaw" />

    <TextView
        android:id="@+id/descac5"
        android:layout_width="336dp"
        android:layout_height="313dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="36dp"
        android:layout_marginStart="38dp"
        android:fontFamily="casual"
        android:text="TextView"
        android:textAlignment="textStart"
        android:textColor="#0d0503"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="5dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="179dp"
        android:layout_height="164dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="33dp"
        android:alpha="0.8"
        app:srcCompat="@drawable/dinosour" />

    <TextView
        android:id="@+id/descripac5"
        android:layout_width="365dp"
        android:layout_height="53dp"
        android:layout_above="@+id/descac5"
        android:layout_centerHorizontal="true"
        android:fontFamily="serif-monospace"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#8d170a"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="5dp" />

</RelativeLayout>